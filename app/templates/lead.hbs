<div class="panel-heading">
	<div class="search">
		<input type="text " placeholder="Search by Lead Name" >
	</div>
	<a href="">
		<div class="sr-only">Add New Lead</div>
	</a>
</div>

<div class="panel-content">
	<h3 class="panel-title">Leads</h3>

	<ul class="collection">
		{{#each model.data as |lead|}}
			<Lead-list-item
			  @first={{lead.attributes.first}}
			  @last={{lead.attributes.last}}
			  @email={{lead.attributes.email}}
			/>
		{{/each}}
	</ul>
</div>